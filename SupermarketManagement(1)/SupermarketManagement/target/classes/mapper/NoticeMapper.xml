<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace命名空间,在里面可以自定义标签-->
<mapper namespace="mapper.NoticeMapper">
    
    <resultMap id="noticeResultMap" type="notice">
        <result column="n_id" property="nId"></result>
        <result column="n_time" property="nTime"></result>
    </resultMap>


</mapper>
